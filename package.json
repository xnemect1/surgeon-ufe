{
  "name": "surgeon-ufe",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/surgeon-ufe/surgeon-ufe.esm.js",
  "exports": {
    ".": {
      "import": "./dist/surgeon-ufe/surgeon-ufe.esm.js",
      "require": "./dist/surgeon-ufe/surgeon-ufe.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "convert-openapi": "js-yaml  ./api/surgeon-wl.openapi.yaml > .openapi.json",
    "mock-api": "open-api-mocker --schema .openapi.json --port 5000",
    "start:app": "stencil build --dev --watch --serve",
    "start:mock": "run-s convert-openapi mock-api",
    "start": "run-p -r start:mock start:app",
    "build": "stencil build",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "openapi": "openapi-generator-cli generate"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "open-api-mocker": "^2.0.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@material/web": "^1.4.1",
    "axios": "^1.6.0"
  }
}
